class Solution {
    public boolean containsDuplicate(int[] nums) {
        if(nums == null || nums.length == 0){
            return false;
        }

        HashSet<Integer> set = new HashSet<>();
        for(int num:nums){
            set.add(num);
        }
        return set.size() == nums.length ? false : true;
    }
}

//Time complexity: O(N)
//Space complexity: O(N)
