class Solution {
// 时间复杂度：O(n)
// 空间复杂度：O(n)
    public int fib(int n) {
        if(n < 1){
            return 0;
        }
        if(n == 1 | n == 2){
            return 1;
        }
        
        int prev = 1;
        int curr = 1;
        for(int i = 3; i <= n; i++){
            int sum = prev + curr;
            prev = curr;
            curr = sum;
        }
        return curr;
    }
}

https://leetcode-cn.com/problems/fibonacci-number/solution/dong-tai-gui-hua-tao-lu-xiang-jie-by-labuladong/
