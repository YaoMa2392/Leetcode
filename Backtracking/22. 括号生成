class Solution {
    public List<String> generateParenthesis(int n) {
        List<String> result = new ArrayList<>();
        backtracking(n, result, 0, 0, "");
        return result;
    }

    private void backtracking (int n, List<String> result, int left, int right, String cur){
        if(right > left){
            return;
        }
        if(left == n && right == n){
            result.add(cur);
            return;
        }
        if(left < n){
            backtracking(n, result, left + 1, right, cur + "(");
        }
        if(right < left){
            backtracking(n, result, left, right + 1, cur + ")");
        }
    }
}

// Backtracking
// Time Complexity: O(n)
// Spcace Complexity: O(n)
